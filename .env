# .env file

# [Security Configuration] API access key to protect the service. Please change to your own complex key.
# If left empty or set to "1", no security verification will be performed.
API_MASTER_KEY=1

# [Deployment Configuration] Port the service listens on
PORT=8088

# [Session Management] Cache time for conversation history in memory (seconds), default 1 hour
SESSION_CACHE_TTL=3600
